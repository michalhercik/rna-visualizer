{
  "name": "rnavis-demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "copy": "copyfiles -u 1 \"./src/**/*.{html, css}\" \"./dist/\"",
    "rm-dist": "del-cli --force ./dist",
    "rm-modules": "del-cli --force ./node_modules",
    "compile": "tsc --build && npm run copy",
    "build": "npm install && npm link rna-visualizer && npm run compile",
    "clean": "npm run rm-dist && npm run rm-modules"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0"
  }
}
