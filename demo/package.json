{
  "name": "rnavis-demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "copy": "copyfiles -u 1 \"./src/**/*.html\" \"./dist/\" && copyfiles -u 1 \"./src/**/*.css\" \"./dist/\" && copyfiles -u 1 \"./src/**/*.js\" \"./dist/\"",
    "rm-dist": "del-cli --force ./dist",
    "compile": "esbuild src/main.ts --bundle --sourcemap --outdir=dist --global-name=main",
    "build": "npm run compile && npm run copy",
    "clean": "npm run rm-dist",
    "rebuild": "npm run clean && npm run build"
  },
  "author": "Michal Hercik",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "esbuild": "0.17.10"
  }
}
